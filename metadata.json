{
  "name": "simp-tuned",
  "version": "0.0.1",
  "author": "SIMP Team",
  "summary": "A SIMP Puppet module for managing tuned/ktune",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-tuned",
  "project_page": "https://github.com/simp/pupmod-simp-tuned",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "ktune"
  ],
  "dependencies": [
